class poly
{
	public int add(int x, int y) 
	{ return( x + y); 
	}
public double add(double x, double y) 
{
	return( x + y);
	}
public static void main(String[] args)
{
	poly a = new poly() ;
	System.out.println("Integer Addition "+ a.add(4,3) );
	System.out.println("Floating point Addition " +a.add(4.0,3.0) );
	}
	}
